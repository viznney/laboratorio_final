<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="entidades.EstadoTipoImpuesto" table="estadotipoimpuesto">

        <id name="OID" type="string">
            <column name="OIDEstadoTipoImpuesto"/>
            <generator class="uuid2"/>
        </id>

        <property name="codigoEstadoTipoImpuesto" type="int">
            <column name="codigo"/>
        </property>

        <property name="nombreEstadoTipoImpuesto" type="string">
            <column name="nombre"/>
        </property>

    </class>

</hibernate-mapping>
